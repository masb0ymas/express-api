{
  "name": "express-api",
  "version": "0.0.2",
  "description": "A simple Express API with TypeScript",
  "main": "src/main.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "secret": "chmod +x ./script/secret.sh && bash ./script/secret.sh",
    "start": "nodemon --exec node ./dist/main.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc && tsc-alias",
    "dev": "concurrently \"npm run build:watch\" \"npm run watch-dev\"",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/main.js",
    "start:staging": "NODE_ENV=staging pm2-runtime ./dist/main.js",
    "start:production": "NODE_ENV=production pm2-runtime ./dist/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prerelease": "npm-run-all prebuild",
    "postrelease": "git push --follow-tags origin main",
    "release": "release-it",
    "release:pre": "release-it --preRelease=beta",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major"
  },
  "keywords": [
    "express",
    "api",
    "typescript"
  ],
  "author": "masb0ymas <me@masb0ymas.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20.x"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "compression": "^1.8.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "lodash": "^4.17.21",
    "multer": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pino": "^9.11.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "pm2-runtime": "^5.4.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "release-it": "^18.1.2",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  }
}
