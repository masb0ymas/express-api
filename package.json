{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "nodemon --exec node ./dist/main.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc && tsc-alias",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "dev:watch": "concurrently \"npm run build:watch\" \"npm run watch-dev\"",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/main.js",
    "start:staging": "NODE_ENV=staging pm2-runtime ./dist/main.js",
    "start:production": "NODE_ENV=production pm2-runtime ./dist/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20.x"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "compression": "^1.8.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "npm-run-all": "^4.1.5",
    "pg": "^8.14.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "pm2-runtime": "^5.4.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.11",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  }
}
